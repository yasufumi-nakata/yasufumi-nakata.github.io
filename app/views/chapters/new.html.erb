<% content_for :header do %>
  <%= javascript_include_tag 'skipembedded/ckeditor/ckeditor.js' %>
<% end -%>

<% content_for :contents_right do %>
  <%= render :partial => 'wiki/navigation' %>
<% end -%>

<% content_for :contents_left do -%>
  <%= render :partial => "chapters/first_half_of_chapters", :locals => {:page=>@current_page, :num=>@before_num} %>
  <%= render :partial => "chapters/page_content", :locals => {:page=>@current_page, :url=>wiki_chapters_path(@current_page.title), :option=>{}} %>
  <%= render :partial => "chapters/latter_half_of_chapters", :locals => {:page=>@current_page, :num=>@after_num} %>
  <%= render :partial => "chapters/embed_dialog" %>
<% end -%>

