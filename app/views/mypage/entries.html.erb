<% content_for :stylesheet do -%>
<style type="text/css">
  div#antenna_navi span.item {
    border-right: 1px solid #555555;
    padding-right: 3px;
    margin-right: 7px;
  }
</style>
<% end -%>

<% content_for :contents_left do -%>
  <%= render :partial => 'contents_left_base' %>
<% end -%>

<% content_for :contents_right do -%>
  <%= i18n_will_paginate_with_container_wrapper @entries %>
  <%= render :partial => "entries/topix_entries",
    :locals => {
      :title => @title_name,
      :entries => @entries,
      :symbol2name_hash => @symbol2name_hash
    }
  %>
  <%= i18n_will_paginate_with_container_wrapper @entries %>
<% end -%>
